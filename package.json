{
  "name": "oapi-litegraph-nodegen",
  "version": "1.0.0",
  "description": "Generate dynamic and callable LiteGraph nodes from OpenAPI specifications, enabling seamless API integration in a visual programming environment",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist demo/dist node_modules/.cache && npm cache clean --force",
    "start:demo": "cd demo && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"demo/**/*.{ts,js}\"",
    "build:prod": "tsc && NODE_ENV=production",
    "build:dev": "tsc && NODE_ENV=development",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx,jsx,json,css,scss,md}\""
  },
  "keywords": [
    "litegraph",
    "openapi",
    "swagger",
    "node-generator",
    "visual-programming",
    "api-integration"
  ],
  "author": "Rick Staa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen"
  },
  "bugs": {
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "c8": "^8.0.1",
    "esbuild": "^0.24.0",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^8.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "nock": "^13.5.6",
    "prettier": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "litegraph.js": "^0.7.18",
    "loglevel": "^1.9.2",
    "openapi-client-axios": "^7.5.5"
  }
}

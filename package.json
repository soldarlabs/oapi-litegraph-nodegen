{
  "name": "oapi-litegraph-nodegen",
  "version": "1.0.0",
  "description": "Generate dynamic and callable LiteGraph nodes from OpenAPI specifications, enabling seamless API integration in a visual programming environment",
  "keywords": [
    "litegraph",
    "openapi",
    "swagger",
    "node-generator",
    "visual-programming",
    "api-integration"
  ],
  "bugs": {
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen"
  },
  "license": "MIT",
  "author": "Rick Staa",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "clean": "rimraf dist node_modules/.cache && npm cache clean --force",
    "dev": "NODE_ENV=development tsup",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,jsx,json,css,scss,md}'",
    "lint": "eslint . --ext .ts,.js",
    "sort-package-json": "sort-package-json",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "litegraph.js": "^0.7.18",
    "loglevel": "^1.9.2",
    "openapi-client-axios": "^7.5.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "c8": "^8.0.1",
    "eslint": "^9.16.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "nock": "^13.5.6",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "sort-package-json": "^2.12.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

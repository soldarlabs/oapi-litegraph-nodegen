{
  "name": "oapi-litegraph-nodegen",
  "version": "1.0.0",
  "description": "Generate dynamic and callable LiteGraph nodes from OpenAPI specifications, enabling seamless API integration in a visual programming environment",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist demo/dist node_modules/.cache && npm cache clean --force",
    "start:demo": "cd demo && npm install && npm run build && npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"demo/**/*.{ts,js}\"",
    "build:prod": "NODE_ENV=production tsup",
    "build:dev": "NODE_ENV=development tsup",
    "prettier": "prettier --write \"src/**/*.{js,ts,tsx,jsx,json,css,scss,md}\" \"**/*.{md}\"",
    "deps:update": "ncu -u && npm install",
    "deps:check": "ncu"
  },
  "keywords": [
    "litegraph",
    "openapi",
    "swagger",
    "node-generator",
    "visual-programming",
    "api-integration"
  ],
  "author": "Rick Staa",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen"
  },
  "bugs": {
    "url": "https://github.com/soldarlabs/oapi-litegraph-nodegen/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "c8": "^10.1.2",
    "esbuild": "^0.24.0",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "eslint": "^9.16.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jsdom": "^25.0.1",
    "nock": "^13.5.6",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "litegraph.js": "^0.7.18",
    "loglevel": "^1.9.2",
    "openapi-client-axios": "^7.5.5"
  }
}
